 'use strict'
 
 const setError = require('../error/index').setError

 class Lib {
   before () {
     setError('before')
   }
 
   beforeEach () {
     setError('beforeEach')
   }
 
   after () {
     setError('after')
   }
 
   createWallet () {
     setError('createWallet')
   }
 }
 
 module.exports = Lib
 